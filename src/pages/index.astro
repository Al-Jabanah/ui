---
import Layout from '@layouts/Layout.astro';
import TheHeader from '@components/TheHeader.vue';
import TheFeatures from '@components/TheFeatures.vue';
import ThePricing from '@components/ThePricing.vue';
import TheFAQ from '@components/TheFAQ.vue';
import TheBlog from '@components/TheBlog.vue';
import TheContact from '@components/TheContact.vue';
import TheFooter from '@components/TheFooter.vue';
---

<Layout title="Welcome to Astro.">
	<main>
		<TheHeader client:load />
		<TheFeatures client:visible />
		<ThePricing client:visible />
		<TheFAQ client:visible />
		<TheBlog client:visible />
		<TheContact client:visible />
		<TheFooter client:visible />
	</main>
</Layout>
